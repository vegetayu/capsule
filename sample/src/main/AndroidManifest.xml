<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capsule.sample">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">

    <activity android:name="com.capsule.sample.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name=".func.empty.EmptyActivity" />
    <activity android:name=".func.head.HeadActivity" />
    <activity android:name=".func.load.LoadActivity" />
    <activity android:name=".func.click.ClickActivity" />
    <activity android:name=".func.divider.DividerActivity" />
    <activity android:name=".func.multi.MultipleActivity" />
    <activity android:name=".func.foot.FootActivity" />
  </application>

</manifest>